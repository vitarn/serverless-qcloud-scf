{
    "name": "serverless-qcloud-scf",
    "version": "0.0.4",
    "description": "Provider plugin for the Serverless Framework v1.x which adds support for Qcloud SCF(Serverless Cloud Function).",
    "main": "index.js",
    "author": "colder",
    "license": "MIT",
    "repository": {
        "git": "https://github.com/vitarn/serverless-qcloud-scf"
    },
    "scripts": {
        "test": "jest",
        "testing": "jest --watch",
        "coverage": "jest --maxWorkers=4 --coverage",
        "typecheck": "tsc --noEmit",
        "release": "standard-version",
        "prepack": "tsc --removeComments --inlineSourceMap false | echo shut up"
    },
    "homepage": "https://github.com/vitarn/serverless-qcloud-scf",
    "keywords": [
        "serverless",
        "serverless framework",
        "serverless applications",
        "serverless modules",
        "serverless cloud function",
        "qcloud",
        "qcloud scf",
        "iot",
        "internet of things",
        "serverless.com"
    ],
    "dependencies": {
        "async": "^2.6.2",
        "bluebird": "^3.5.3",
        "chalk": "^2.4.2",
        "cos-nodejs-sdk-v5": "^2.5.6",
        "fs-extra": "^7.0.1",
        "ini": "^1.3.5",
        "lodash": "^4.17.11",
        "qcloud-apigateway": "^0.0.7",
        "qcloudapi-sdk": "^0.2.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.5",
        "@types/jest": "^24.0.6",
        "@types/lodash": "^4.14.121",
        "@types/node": "^11.9.4",
        "@types/sinon": "^7.0.6",
        "jest": "^24.1.0",
        "sinon": "^7.2.4",
        "standard-version": "^5.0.0",
        "ts-jest": "^24.0.0",
        "typescript": "^3.3.3"
    },
    "engines": {
        "node": ">=4"
    },
    "files": [
        "**/*.js",
        "**/*.json",
        "types/**/*.d.ts",
        "!.vscode",
        "!test",
        "!*.test.js",
        "!tsconfig*.json",
        "!yarn.lock"
    ],
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/*.test.ts"
        ]
    }
}
